{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "psbt",
    "tx",
    "txid"
  ],
  "properties": {
    "psbt": {
      "type": "string",
      "description": "the completed PSBT representing the signed transaction"
    },
    "tx": {
      "type": "hex",
      "description": "the fully signed transaction"
    },
    "txid": {
      "type": "txid",
      "description": "the transaction id of *tx*"
    }
  }
}
