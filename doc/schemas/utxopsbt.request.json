{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "satoshi",
    "feerate",
    "startweight",
    "utxos"
  ],
  "properties": {
    "satoshi": {
      "type": "msat"
    },
    "feerate": {
      "type": "feerate"
    },
    "startweight": {
      "type": "u32"
    },
    "utxos": {
      "type": "array",
      "items": {
        "type": "outpoint"
      }
    },
    "reserve": {
      "type": "u32",
      "description": "reserve is a number: if non-zero number then reserveinputs is called (successfully, with exclusive true) on the returned PSBT for this number of blocks (default: 72)."
    },
    "reservedok": {
      "type": "boolean"
    },
    "locktime": {
      "type": "u32"
    },
    "min_witness_weight": {
      "type": "u32"
    },
    "excess_as_change": {
      "type": "boolean"
    }
  }
}
