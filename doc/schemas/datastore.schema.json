{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "required": [ "key", "hex" ],
    "properties": {
        "key": {
            "type": "string",
            "description": "The key which has been added to the datastore"
        },
        "hex": {
            "type": "hex",
            "description": "The hex data which has been added to the datastore"
        }
    }
}
