{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "psbt"
  ],
  "properties": {
    "psbt": {
      "type": "string",
      "description": "the psbt to be signed"
    },
    "signonly": {
      "type": "array",
      "description": "input numbers to sign",
      "items": {
        "type": "u32"
      }
    }
  }
}
