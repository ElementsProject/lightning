{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "outputs"
  ],
  "properties": {
    "outputs": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": true,
        "patternProperties": {
          "^[A-Za-z]*$": {
            "type": "msat_or_all",
            "description": "pair of (destination -> amount)"
          }
        }
      }
    },
    "feerate": {
      "type": "feerate"
    },
    "minconf": {
      "type": "u32"
    },
    "utxos": {
      "type": "outpoint"
    }
  }
}
