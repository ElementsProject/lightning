{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "psbt",
    "version"
  ],
  "properties": {
    "psbt": {
      "type": "string",
      "description": "the PSBT to change versions"
    },
    "version": {
      "type": "u32",
      "description": "the version to set"
    }
  }
}
