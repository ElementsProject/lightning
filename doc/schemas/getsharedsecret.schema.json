{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "required": [
    "shared_secret"
  ],
  "properties": {
    "shared_secret": {
      "type": "hex",
      "description": "the SHA-2 of the compressed encoding of the shared secp256k1 point",
      "maxLength": 64,
      "minLength": 64
    }
  }
}
