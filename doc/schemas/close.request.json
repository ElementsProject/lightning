{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "id"
  ],
  "properties": {
    "id": {
      "type": "string",
      "description": "peer id, channel id or short_channel_id"
    },
    "unilateraltimeout": {
      "type": "u32",
      "description": ""
    },
    "destination": {
      "type": "string",
      "description": ""
    },
    "fee_negotiation_step": {
      "type": "string",
      "description": ""
    },
    "wrong_funding": {
      "type": "outpoint",
      "description": ""
    },
    "force_lease_closed": {
      "type": "boolean",
      "description": ""
    },
    "feerange": {
      "type": "array",
      "items": {
        "type": "feerate"
      },
      "description": ""
    }
  }
}
