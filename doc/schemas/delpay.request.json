{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "payment_hash",
    "status"
  ],
  "properties": {
    "payment_hash": {
      "type": "hash",
      "description": "payment hash of the payment to delete."
    },
    "status": {
      "type": "string",
      "enum": [
        "complete",
        "failed"
      ],
      "description": "status of the payment we wish to delete"
    }
  }
}
