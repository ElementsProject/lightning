{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "destination"
  ],
  "properties": {
    "destination": {
      "type": "pubkey"
    },
    "satoshi": {
      "type": "msat|all"
    },
    "feerate": {
      "type": "feerate"
    },
    "minconf": {
      "type": "u16"
    },
    "utxos": {
      "type": "array",
      "items": {
        "type": "utxo"
      }
    }
  }
}
